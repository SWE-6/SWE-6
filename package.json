{
    "name": "react-redux-template",
    "description": "An example app using the MERN stack, with the main branch using React and Redux.",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=10.14.2",
        "npm": ">=6.4.1",
        "yarn": ">=1.22.5"
    },
    "scripts": {
        "startDev": "cd ./server && yarn server && cd ../client && yarn start",
        "postinstall": "(cd server && yarn install); (cd client && yarn install)"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@upstatement/prettier-config": "^0.3.0"
    },
    "dependencies": {
        "axios": "^0.26.0",
        "bootstrap": "^5.1.3",
        "express-async-handler": "^1.2.0",
        "node-notifier": "^8.0.1",
        "object-path": "^0.11.5",
        "react-router-dom": "^6.2.1",
        "react-table": "^7.7.0",
        "styled-components": "^5.3.3"
    }
}
